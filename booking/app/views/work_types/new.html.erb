
<div class = "row">
  <div class = "col-md-4 col-md-offset-4">
    <h3>Create a new Work Type</h3>
  <%= form_for @worktype do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control", autofocus: true %>
    <%= f.submit "Submit", class: "btn btn-primary" %>
  <% end %>
  </div>
</div>
